<?xml version="1.0"?>
<package>
	<metadata>
		<id>
			{{ params.id }}
		</id>
		<version>
			{{ params.version }}
		</version>
		<description>
			{{ params.description }}
		</description>
		<tags>
			{{ params.tags }}
		</tags>
		<authors>
			{{ params.authors}}
		</authors>
	</metadata>
	<files>
		<file src="{{ params.directory }}\**\*.*" target="build\native\include" />

	</files>
</package>